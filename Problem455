class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        s.sort(); g.sort(); i = 0; j = 0; numContent = 0 
        while i < len(s) and j < len(g) and s[i] >= g[j]:
            numContent += 1; i += i; j += 1 
        return numContent
            
            
        